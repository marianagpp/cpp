    if (aio%2==0){
        if (mes==1 && mes==2 && mes==3){
            if (dia%2==0){
                cout<<"Tu angel es Miguel";
            }
            else{
                cout<<"Tu angel es Gabriel";
            }

        }
        if (mes==4 && mes==5 && mes==6){
            if (dia%2==0){
                cout<<"Tu angel es Jofiel";
            }
            else{
                cout<<"Tu angel es Raziel";
            }

        }
        if (mes==7 && mes==8 && mes==9){
            if (dia%2==0){
                cout<<"Tu angel es Chamuel";
            }
            else{
                cout<<"Tu angel es Selatiel";
            }

        }
        if (mes==10 && mes==11 && mes==12){
            if (dia%2==0){
                cout<<"Tu angel es Ariel";
            }
            else{
                cout<<"Tu angel es Jeremiel";
            }

        }
    }
    else{
        if (mes==1 && mes==2 && mes==3){
            if (dia%2==0){
                cout<<"Tu angel es Rafael";
            }
            else{
                cout<<"Tu angel es Uriel";
            }

        }
